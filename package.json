{
  "name": "@vivgrid/vercel-ai-sdk-provider",
  "version": "0.1.0",
  "description": "Vercel AI SDK provider for vivgrid AI Bridge platform",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "lint": "biome check --write .",
    "lint:check": "biome check .",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "check": "biome check --write .",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "release": "pnpm run build && pnpm run changeset:publish",
    "prepublishOnly": "pnpm run clean && pnpm run check && pnpm run build"
  },
  "keywords": [
    "ai",
    "vercel",
    "ai-sdk",
    "vivgrid",
    "llm",
    "language-model"
  ],
  "author": "vivgrid",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/provider": "1.1.3",
    "@ai-sdk/provider-utils": "2.2.8",
    "ai": "4.3.16",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.0.6",
    "@changesets/cli": "^2.27.9",
    "@types/node": "24.0.8",
    "tsup": "^8.5.0",
    "tsx": "4.20.3",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.12.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/vivgrid/vercel-ai-sdk-provider"
  },
  "homepage": "https://www.vivgrid.com"
}
